<T3DataStructure>
<sheets>	
	<MAIN>
		<ROOT>
			<TCEforms>
				<sheetTitle>LLL:EXT:ke_yac/pi1/locallang.php:ff_main</sheetTitle>
			</TCEforms>
			<type>array</type>
			<el>
				<mode_selector>
					<TCEforms>
						<label>LLL:EXT:ke_yac/pi1/locallang.php:ff_mode_selector</label>
						<config>
							<type>select</type>
							<items>
								<numIndex index="0">
									<numIndex index="0">LLL:EXT:ke_yac/pi1/locallang.xml:ff_mode.I.0</numIndex>
									<numIndex index="1">0</numIndex>
								</numIndex>
								<numIndex index="1">
									<numIndex index="0">LLL:EXT:ke_yac/pi1/locallang.xml:ff_mode.I.1</numIndex>
									<numIndex index="1">1</numIndex>
								</numIndex>
								<numIndex index="2">
									<numIndex index="0">LLL:EXT:ke_yac/pi1/locallang.xml:ff_mode.I.2</numIndex>
									<numIndex index="1">2</numIndex>
								</numIndex>
							</items>
							<size>1</size>
						</config>
					</TCEforms>
				</mode_selector>
				
				<templateFile>
					<TCEforms type="array">
	                    <label>LLL:EXT:ke_yac/pi1/locallang.php:ff_templateFile</label>
						<config type="array">
	                        <type>group</type>
	                        <internal_type>file</internal_type>
	                        <allowed>htm,html,tmpl</allowed>
	                        <max_size>1000</max_size>
	                        <uploadfolder>uploads/tx_keyac</uploadfolder>
	                        <size>1</size>
	                        <maxitems>1</maxitems>
	                        <minitems>0</minitems>
	                    </config>
	                </TCEforms>
				</templateFile>
				
			</el>
		</ROOT>
	</MAIN>
	
	<CALENDAR>
		<ROOT>
			<TCEforms>
				<sheetTitle>LLL:EXT:ke_yac/pi1/locallang.php:ff_calendar</sheetTitle>
			</TCEforms>
			<type>array</type>
			<el>
				
				<columns>
					<TCEforms>
						<label>LLL:EXT:ke_yac/pi1/locallang.php:ff_columns</label>
						<config>
							<type>input</type>
							<size>3</size>
							<eval>int</eval>
							<range type="array">
								<upper>12</upper>
								<lower>0</lower>
							</range>
						</config>
					</TCEforms>
				</columns>
				
				
				<rows>
					<TCEforms>
						<label>LLL:EXT:ke_yac/pi1/locallang.php:ff_rows</label>
						<config>
							<type>input</type>
							<size>3</size>
							<eval>int</eval>
							<range type="array">
								<upper>12</upper>
								<lower>0</lower>
							</range>
						</config>
					</TCEforms>
				</rows>
				
				
				
				
				
				<!-- Show day of week-->
				<showDaysRow>
					<TCEforms>
						<label>LLL:EXT:ke_yac/pi1/locallang.php:ff_showDaysRow</label>
						<config>
							<type>check</type>
						</config>
					</TCEforms>
				</showDaysRow>
				<!-- Show number of week -->
				<showWeekNumberRow>
					<TCEforms>
						<label>LLL:EXT:ke_yac/pi1/locallang.php:ff_showWeekNumberRow</label>
						<config>
							<type>check</type>
						</config>
					</TCEforms>
				</showWeekNumberRow>
				<!-- months-navigation -->
				<showMonthsNavigation>
					<TCEforms>
						<label>LLL:EXT:ke_yac/pi1/locallang.php:ff_showMonthsNavigation</label>
						<config>
							<type>input</type>
							<size>3</size>
							<eval>tx_yacevaluations</eval>
						</config>
					</TCEforms>
				</showMonthsNavigation>
				<!-- Hide Calendar-->
				<showHideCalendarLink>
					<TCEforms>
						<label>LLL:EXT:ke_yac/pi1/locallang.php:ff_showHideCalendarLink</label>
						<config>
							<type>check</type>
						</config>
					</TCEforms>
				</showHideCalendarLink>
				<!-- Show Legend -->
				<showLegend>
					<TCEforms>
						<label>LLL:EXT:ke_yac/pi1/locallang.php:ff_showLegend</label>
						<config>
							<type>check</type>
						</config>
					</TCEforms>
				</showLegend>
				<!-- Show List-View -->
				<showList>
					<TCEforms>
						<label>LLL:EXT:ke_yac/pi1/locallang.php:ff_showList</label>
						<config>
							<type>check</type>
						</config>
					</TCEforms>
				</showList>
				<!-- LinkToSingleView -->
				<linkToSingleView>
					<TCEforms>
						<label>LLL:EXT:ke_yac/pi1/locallang.php:ff_linkToSingleView</label>
						<config>
							<type>check</type>
						</config>
					</TCEforms>
				</linkToSingleView>
				
				<!-- show events without category -->
				<showEventsWithoutCat>
					<TCEforms>
						<label>LLL:EXT:ke_yac/pi1/locallang.php:ff_showEventsWithoutCat</label>
						<config>
							<type>check</type>
						</config>
					</TCEforms>
				</showEventsWithoutCat>
				
			</el>
		</ROOT>
	</CALENDAR>
	
	<TOOLTIPS>
		<ROOT>
			<TCEforms>
				<sheetTitle>LLL:EXT:ke_yac/pi1/locallang.php:ff_tooltips</sheetTitle>
			</TCEforms>
			<type>array</type>
			<el>
				<!-- Show tooltips -->
				<showTooltips>
					<TCEforms>
						<label>LLL:EXT:ke_yac/pi1/locallang.php:ff_showTooltips</label>
						<config>
							<type>check</type>
						</config>
					</TCEforms>
				</showTooltips>
				
				<!-- Duration of tooltip in ms  -->
				<tooltipDuration>
					<TCEforms>
						<label>LLL:EXT:ke_yac/pi1/locallang.php:ff_tooltipDuration</label>
						<config>
							<type>input</type>
							<size>5</size>
							<eval>int</eval>
							<range type="array">
								<upper>99999</upper>
								<lower>0</lower>
							</range>
						</config>
					</TCEforms>
				</tooltipDuration>
			</el>
		</ROOT>
	</TOOLTIPS>
	
	<TEASER>
		<ROOT>
			<TCEforms>
				<sheetTitle>LLL:EXT:ke_yac/pi1/locallang.php:ff_teaser</sheetTitle>
			</TCEforms>
			<type>array</type>
			<el>
				<!-- single pid for linking from teaser view to list view -->
				<singlePid>
					<TCEforms>
						<exclude>1</exclude>
						<label>LLL:EXT:ke_yac/pi1/locallang.xml:ff_singlePid</label>
						<config>
							<type>group</type>
							<internal_type>db</internal_type>
							<allowed>pages</allowed>
							<size>1</size>
							<maxitems>1</maxitems>
							<minitems>0</minitems>
							<show_thumbs>1</show_thumbs>
						</config>
					</TCEforms>
				</singlePid>
				<!-- headertext of teaser view -->
				<teaserHeader>
					<TCEforms>
						<label>LLL:EXT:ke_yac/pi1/locallang.xml:ff_teaserHeader</label>
						<config>
							<type>input</type>
							<size>30</size>
						</config>
					</TCEforms>
				</teaserHeader>
				<!-- how many items are shown in teaser-view -->
				<teaserLimit>
					<TCEforms>
						<label>LLL:EXT:ke_yac/pi1/locallang.xml:ff_teaserLimit</label>
						<config>
							<type>input</type>
							<size>10</size>
						</config>
					</TCEforms>
				</teaserLimit>
				<!-- lenght of item title (crop title at this length) -->
				<teaserLength>
					<TCEforms>
						<label>LLL:EXT:ke_yac/pi1/locallang.xml:ff_teaserLength</label>
						<config>
							<type>input</type>
							<size>10</size>
						</config>
					</TCEforms>
				</teaserLength>
				
			</el>
		</ROOT>
	</TEASER>
	
	<DATEFORMAT>
		<ROOT>
			<TCEforms>
				<sheetTitle>LLL:EXT:ke_yac/pi1/locallang.php:ff_dateFormat</sheetTitle>
			</TCEforms>
			<type>array</type>
			<el>
				<strftimeFormatStringWithTime>
					<TCEforms>
						<label>LLL:EXT:ke_yac/pi1/locallang.xml:ff_strftimeFormatStringWithTime</label>
						<config>
							<type>input</type>
							<size>20</size>
						</config>
					</TCEforms>
				</strftimeFormatStringWithTime>
				
				<strftimeFormatStringWithoutTime>
					<TCEforms>
						<label>LLL:EXT:ke_yac/pi1/locallang.xml:ff_strftimeFormatStringWithoutTime</label>
						<config>
							<type>input</type>
							<size>20</size>
						</config>
					</TCEforms>
				</strftimeFormatStringWithoutTime>
				
				<strftimeFormatTime>
					<TCEforms>
						<label>LLL:EXT:ke_yac/pi1/locallang.xml:ff_strftimeFormatTime</label>
						<config>
							<type>input</type>
							<size>20</size>
						</config>
					</TCEforms>
				</strftimeFormatTime>
				
			</el>
		</ROOT>
	</DATEFORMAT>
	
	<MYEVENTS>
		<ROOT>
			<TCEforms>
				<sheetTitle>LLL:EXT:ke_yac/pi1/locallang.php:ff_myEvents</sheetTitle>
			</TCEforms>
			<type>array</type>
			<el>
				<myEventsSinglePid>
					<TCEforms>
						<exclude>1</exclude>
						<label>LLL:EXT:ke_yac/pi1/locallang.xml:ff_singlePid</label>
						<config>
							<type>group</type>
							<internal_type>db</internal_type>
							<allowed>pages</allowed>
							<size>1</size>
							<maxitems>1</maxitems>
							<minitems>0</minitems>
							<show_thumbs>1</show_thumbs>
						</config>
					</TCEforms>
				</myEventsSinglePid>
				
			</el>
		</ROOT>
	</MYEVENTS>
	
</sheets>
</T3DataStructure>